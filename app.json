{
  "expo": {
    "name": "sovrn-mobile",
    "slug": "sovrn-mobile",
    "scheme": "sovrnmobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "53.0.0",
    "platforms": ["ios", "android", "web"],
    "backgroundColor": "#000000",
    "primaryColor": "#000000",
    "userInterfaceStyle": "dark",
    "splash": {
      "backgroundColor": "#000000",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    }
  }
}
